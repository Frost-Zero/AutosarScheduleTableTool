<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<Group xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EPCellController">
   <children>
      <VBox onMouseClicked="#onEPClick" prefHeight="150.0" prefWidth="80.0" style="-fx-background-color: #aaaaaa;">
         <children>
            <Label fx:id="labelEPCId" alignment="CENTER" contentDisplay="CENTER" prefHeight="80.0" prefWidth="80.0" style="-fx-border-color: #eeeeee; -fx-border-width: 1; -fx-background-color: #FFFFFF;" text="EP1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Label>
            <Pane prefHeight="70.0" prefWidth="80.0">
               <children>
                  <Label fx:id="labelEPCOffset" alignment="CENTER" layoutX="10.0" layoutY="34.0" prefWidth="60.0" text="0">
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label layoutX="21.0" layoutY="12.0" text="Offset" />
               </children>
            </Pane>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </VBox>
      <Rectangle fill="#aaaaaa" height="50.0" layoutX="39.0" layoutY="150.0" onMouseClicked="#onEPClick" stroke="BLACK" strokeMiterLimit="1.0" strokeType="INSIDE" strokeWidth="0.0" width="2.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Rectangle>
   </children>
</Group>
